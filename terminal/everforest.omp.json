{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "powerline",
          "foreground": "#d3c6aa",
          "background": "#2b3339",
          "properties": {
            "display_host": false,
            "display_user": true
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "#a7c080",
          "background": "#323c41",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#e67e80",
          "background": "#3a454a",
          "properties": {
            "branch_icon": " ",
            "fetch_status": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#a7c080",
          "properties": {
            "text": "❯"
          }
        }
      ]
    }
  ],
  "transient_prompt": {
    "foreground": "#a7c080",
    "background": "transparent",
    "text": "❯ "
  },
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Folder }}"
}
